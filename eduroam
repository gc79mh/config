nmcli connection add type wifi con-name eduroam ifname wlo1 ssid eduroam \
wifi-sec.key-mgmt wpa-eap 802-1x.eap tls \
802-1x.identity wiktor.bochenski@student.put.poznan.pl \
802-1x.client-cert /etc/wiktor.bochenski@student.put.poznan.pl.p12 \
802-1x.private-key /etc/wiktor.bochenski@student.put.poznan.pl.p12 \
802-1x.private-key-password 12345678 \
802-1x.system-ca-certs no
